{
    "compilerOptions": {
        "allowJs": true,
        "baseUrl": ".",
        "declaration": true,
        "declarationMap": true,
        "emitDeclarationOnly": true,
        "esModuleInterop": true,
        "jsx": "preserve",
        "lib": ["dom", "ESNext"],
        "module": "ESNext",
        "moduleResolution": "bundler",
        "moduleDetection": "auto",
        "noEmit": true,
        "outDir": "./dist/types",
        "paths": {
            "@oxigraph/*": ["./src/assets/oxigraph/*"]
        },
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "skipDefaultLibCheck": true,
        "strict": true,
        "strictNullChecks": true,
        "target": "ESNext",
        "types": [
            "vite/client"
        ],
        "verbatimModuleSyntax": true
    },
    "include": ["**/*.js", "**/*.ts", "package.json"]
}
